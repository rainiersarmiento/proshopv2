{
  "name": "proshop",
  "version": "1.0.0",
  "description": "A mock eCommerce Application built with MERN Stack",
  "homepage": "https://github.com/rainiersarmiento/proshopv2#readme",
  "bugs": {
    "url": "https://github.com/rainiersarmiento/proshopv2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainiersarmiento/proshopv2.git"
  },
  "license": "ISC",
  "author": "Rainier Sarmiento",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node backend/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "node --watch --env-file=.env backend/server.js",
    "client": "npm run dev --prefix frontend",
    "data:import": "node --env-file=.env backend/seeder.js",
    "data:destroy": "node --env-file=.env backend/seeder.js -d"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11"
  }
}
